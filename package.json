{
  "name": "capitolis",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open --browser chrome",
    "cypress:run": "cypress run --browser chrome --reporter mochawesome --reporter-options reportDir=cypress/reports,json=true,html=false,embeddedScreenshots=true",
    "report:html": "marge cypress/reports/*.json --reportDir=cypress/reports/html --inline",
    "test:all": "npm run cypress:run && npm run report:html"
  },
