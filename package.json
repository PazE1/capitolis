  "devDependencies": {
    "@types/node": "^24.3.1",
    "cypress": "^15.1.0",
    "cypress-mochawesome-reporter": "^4.0.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
